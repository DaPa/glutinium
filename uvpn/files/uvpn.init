START=98

USE_PROCD=1
PROG=/usr/sbin/uvpn

start_service() {
  if [ $(uci_get "uvpn.server.enable") = "1" ] ; then
    #
    procd_open_instance
    procd_set_param command "$PROG" --config_uci=/etc/config/uvpn
    procd_close_instance
  else
    return 1
  fi
}
